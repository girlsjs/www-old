<a name="team"></a>

<div class="team-items team">
  <% var posts = site.posts.filter(function(item) {
      var data = item.categories.data
      return (data.length && data[0].name === 'team')
    });

    posts.data.reverse();
    posts.each(function(item){
  %>

    <article id="post-54" class="col2 team type-team">
      <h4 class="entry-title"><%- item.title %></h4>
      <img src="<%- config.root %>images/team/<%- item.image %>"/>
      <div class="entry-main-content">
        <%- item.content %>
      </div>
    </article>

  <% }); %>
</div>
